<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guanyitong.mapper.BackMoneyDao">

       <!--(分页)查询所有某个用户的某个出借的回款详情-->
      <select id="selectBackMoneyByDealId" parameterType="java.util.Map" resultType="com.guanyitong.model.BackMoney">
           select * from back_money where 1=1
           <if test="dealMoneyId !=null">
                and dealMoneyId = #{dealMoneyId}
           </if>
      </select>
</mapper>
